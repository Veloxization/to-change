<form class="cards-sheet {{cssClass}}" autocomplete="off">
    <header class="sheet-header flexrow">
        <img src="{{document.img}}" data-edit="img" title="{{document.name}}" height="64" width="64"/>
        <h1 class="cardsname"><input name="name" type="text" value="{{document.name}}" placeholder="{{ localize "TC.Cards.FutureDeck" }}"/></h1>
    </header>
    <body>
        <nav class="sheet-navigation tabs" data-group="primary">
            <b><a class="item" data-tab="config">{{ localize "TC.Cards.Config" }}</a></b>
            <b><a class="item active" data-tab="cards">{{ localize "TC.Cards.CardPl" }}</a></b>
        </nav>
    </body>
</form>